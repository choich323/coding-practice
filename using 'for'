// #1
void Quiz_1() {
    int num;
    cin >> num;

    for (int i = 0; i < 9; i++)
        cout << num << " * " << i + 1 << " = " << num * (i + 1) << endl;
}

// #2
void Quiz_2() {
    int test_num, A, B;
    cin >> test_num;

    for (int i = 0; i < test_num; i++) {
        cin >> A >> B;
        cout << A + B << endl;
    }
}

// #3
void Quiz_3() {
    int n, result;
    result = 0;
    cin >> n;
    for (int i = 1; i <= n; i++)
        result = result + i;
    cout << result;
}

// #4
void Quiz_4() {
    int test_num, A, B;
    cin.tie(NULL);
    
    cin >> test_num;

    for (int i = 0; i < test_num; i++) {
        cin >> A >> B;
        cout << A + B << '\n';
    }
}

// #5
void Quiz_5() {
    int n;
    cin >> n;

    for (int i = 0; i < n; i++)
        cout << i + 1 << '\n';
}

// #6
void Quiz_6() {
    int n;
    cin >> n;

    for (int i = n; i > 0; i--)
        cout << i << '\n';
}

// #7
void Quiz_7() {
    int T, A, B;
    cin >> T;

    for (int i = 0; i < T; i++) {
        cin >> A >> B;
        cout << "Case #" << i + 1 << ": " << A + B << '\n';
    }
}

// #8
void Quiz_8(){
    int T, A, B;
    cin >> T;

    for (int i = 0; i < T; i++) {
        cin >> A >> B;
        cout << "Case #" << i + 1 << ": " << A << " + " << B << " = " << A + B << '\n';
    }
}

// #9
void Quiz_9() {
    int N;
    cin >> N;

    for (int i = 0; i < N; i++)
        cout << "*" << '\n';
}


